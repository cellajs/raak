{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV"],
  "globalDependencies": ["config", "backend/.env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["dist/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "seed": {},
    "check:types": {},
    "push": {
      "persistent": true
    },
    "generate": {
      "cache": false,
      "persistent": true
    },
    "generate:pg": {
      "persistent": true
    },
    "studio": {}
  }
}
